{{ suite }}:
    actor: {{ module }}{{ applicationType }}ApiTester
    modules:
        enabled:
            - Asserts
            - \{{ organization }}Test\{{ application }}\{{ module }}\Helper\{{ resource | resourceNameToModelName }}Helper
            - \SprykerTest\Shared\Testify\Helper\Environment
            -
                \SprykerTest\Shared\Testify\Helper\LocatorHelper:
                    projectNamespaces:
                        - {{ organization }}
            - \SprykerTest\Shared\Propel\Helper\TransactionHelper
            - \SprykerTest\Shared\Testify\Helper\ConfigHelper
            - \SprykerTest\Glue\Testify\Helper\DependencyProviderHelper
            - \SprykerTest\Shared\Testify\Helper\DataCleanupHelper
            - \SprykerTest\Glue\Testify\Helper\GlueBackendApiHelper
            - \SprykerTest\Glue\Testify\Helper\JsonPath
            - \{{ organization }}Test\Zed\{{ module | removeApiSuffix }}\Helper\{{ zedDomainEntity }}CrudHelper
            - \SprykerTest\Zed\Testify\Helper\Business\BusinessHelper
